#: ../source/oauth.rst:39
msgid ""
"``reports:write``: grants write access to reports endpoints (*new in "
"0.9.0*),"
msgstr ""

#: ../source/oauth.rst:40
msgid "``users:read``: grants read access to users endpoints,"
msgstr ""

#: ../source/oauth.rst:41
msgid "``users:write``: grants write access to users endpoints,"
